<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.motivity.Admin" table="admin">
<id name="aid" column="aid"></id>
<property name="aname" column ="aname"></property>
<property name="emailid" column ="emailid"></property>
<property name="password" column ="password"></property>
<property name="phone" column ="phone"></property>
<property name="doj" column ="doj"></property>
<property name="dob" column ="dob"></property>
<property name="address" column ="address"></property>
</class>
</hibernate-mapping> 